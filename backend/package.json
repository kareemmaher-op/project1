{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf dist",
    "prestart": "npm run clean && npm run build",
    "start": "func start",
    "test": "jest --runInBand",
    "test:unit": "jest --runInBand tests/unit",
    "test:int": "jest --runInBand tests/integration",
    "deploy": "npm run build && func azure functionapp publish",
    "typeorm:build-ds": "npm run build",
    "typeorm:generate": "npm run typeorm:build-ds && node --loader ts-node/esm ./node_modules/typeorm/cli.js migration:generate src/database/migrations/auto -d ./dist/database/config/data-source.js",
    "typeorm:run": "npm run typeorm:build-ds && node ./node_modules/typeorm/cli.js migration:run -d ./dist/database/config/data-source.js",
    "typeorm:revert": "npm run typeorm:build-ds && node ./node_modules/typeorm/cli.js migration:revert -d ./dist/database/config/data-source.js"
  },
  "dependencies": {
    "@azure/functions": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.26"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.13",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "pg-mem": "^3.0.5",
    "rimraf": "^5.0.0",
    "sql.js": "^1.9.0",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.0.0"
  },
  "main": "dist/index.js"
}
